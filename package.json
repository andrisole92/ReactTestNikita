{
  "name": "react-sass-starterkit",
  "version": "2.4.0",
  "description": "Frontend setup for react with sass and webpack",
  "private": true,
  "scripts": {
    "build": "webpack",
    "build:prod": "webpack -p",
    "delete": "node bin/cli.js delete",
    "multiwatch": "concurrently \"npm run watch\"  \"npm run ssr:watch\" \"npm run start\"",
    "new": "node bin/cli.js new",
    "publish": "webpack && node bin/cli.js publish",
    "publish:prod": "webpack -p && node bin/cli.js publish",
    "publish:only": "node bin/cli.js publish",
    "publish:all": "webpack && node bin/cli.js publishAll",
    "publish:snippet": "webpack && node bin/cli.js publishSnippet",
    "ssr": "babel app --out-dir ./dist/raw --no-babelrc --plugins=transform-class-properties,transform-object-rest-spread,inline-react-svg --presets=react,env",
    "ssr:watch": "babel app --watch --out-dir ./dist/raw --no-babelrc --plugins=transform-class-properties,transform-object-rest-spread,inline-react-svg --presets=react,env",
    "start": "node bin/server.js",
    "test": "mocha internals/test/.setup-test.js app/**/*.test.js",
    "test:single": "mocha internals/test/.setup-test.js",
    "test:watch": "mocha -w internals/test/.setup-test.js app/**/*.test.js",
    "watch": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:frojd/react-sass-starterkit.git"
  },
  "keywords": [
    "webpack",
    "sass",
    "react",
    "frontend",
    "boilerplate",
    "starterkit",
    "cli",
    "staticgenerator"
  ],
  "author": "Daniel Karlsson",
  "license": "MIT",
  "browserslist": [
    "last 2 versions"
  ],
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-inline-react-svg": "^0.5.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.24.0",
    "chai": "^4.1.1",
    "concurrently": "^3.5.0",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.28.4",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "eslint": "^4.7.2",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.4.0",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.5",
    "fs-extra": "^4.0.2",
    "js-beautify": "^1.6.14",
    "jsdom": "^11.2.0",
    "mocha": "^4.0.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "react-test-renderer": "^16.0.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.5.4",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "classnames": "^2.2.5",
    "react": "^16.1.1",
    "react-dom": "^16.1.1"
  }
}
